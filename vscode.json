{
  "window.title": "${dirty} ${activeEditorShort}${separator}${rootName}",
  "window.closeWhenEmpty": false,
  "window.openFilesInNewWindow": "off",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Cobalt2",
  "workbench.sideBar.location": "left",
  "workbench.activityBar.visible": true,
  "workbench.startupEditor": "none",
  "workbench.editor.tabCloseButton": "right",
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "editor.fontWeight": "400",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 2,
  "editor.cursorBlinking": "solid",
  "editor.renderWhitespace": "none",
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.detectIndentation": false,
  "editor.multiCursorModifier": "alt",
  "editor.colorDecorators": true,
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.rulers": [], //[4,8,12,16,20,24,28,32,36,40,44,48,52,56,60,64,68,72,76,80],
  "editor.snippetSuggestions": "top",
  "editor.autoIndent": true,
  "editor.showFoldingControls": "always",
  "editor.find.seedSearchStringFromSelection": true,
  "editor.matchBrackets": true,
  "editor.scrollBeyondLastLine": false,
  "editor.useTabStops": true,
  "editor.parameterHints.enabled": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "eslint.autoFixOnSave": true,
  "prettier.disableLanguages": ["javascript", "javascriptreact"],
  "sass.disableAutoIndent": false,
  "sass.lint.enable": true,
  "vscode_custom_css.imports": ["file:///home/naveen/.vscodestyles.css"],
  "vscode_custom_css.policy": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontWeight": "100",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontWeightBold": "100",
  "extensions.ignoreRecommendations": false,
  "explorer.confirmDelete": false
}
